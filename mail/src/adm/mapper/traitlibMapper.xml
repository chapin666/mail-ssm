<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="adm.mapper.traitlibMapper">
	
<select id="selectTraitlibs" parameterType="traitlib" resultType="traitlib">
	select * from traitlib LIMIT #{start},#{end}
</select>

<select id="traitlibCount" resultType="int">
    select count(*) from traitlib
</select>
</mapper>
